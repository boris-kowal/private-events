<h1>New event</h1>
<%= form_with model: @event do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :place %>
  <%= f.text_field :place %>
  <%= f.label :date %>
  <%= f.date_field :date %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.submit "Submit" %>
<% end %>